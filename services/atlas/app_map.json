{
  "pages": {
    "login": {
      "url": "http://localhost:5173/",
      "identifiers": {
        "url_pattern": "^http://localhost:5173/$",
        "elements": ["button"]
      },
      "description": "Login page with Try Demo button",
      "actions": {
        "login": {
          "description": "Log in using Try Demo button",
          "steps": [
            {
              "type": "click",
              "selector": "button:contains('Try Demo')",
              "fallback_selectors": [
                "//button[contains(text(), 'Try Demo')]",
                "button.btn-primary",
                "form button:first-of-type"
              ],
              "description": "Click Try Demo button"
            },
            {
              "type": "wait",
              "seconds": 2,
              "description": "Wait for login to complete"
            }
          ],
          "navigates_to": "dashboard"
        }
      }
    },
    "dashboard": {
      "url": "http://localhost:5173/",
      "identifiers": {
        "url_pattern": "^http://localhost:5173/$",
        "elements": ["button", ".card"]
      },
      "description": "Main dashboard showing all projects",
      "actions": {
        "navigate_to_create_project": {
          "description": "Navigate to project creation page",
          "steps": [
            {
              "type": "click",
              "selector": "button:contains('New Project')",
              "fallback_selectors": [
                "//button[contains(text(), 'New Project')]",
                "a[href='/create-project']"
              ],
              "description": "Click New Project button in header"
            },
            {
              "type": "wait",
              "seconds": 1
            }
          ],
          "navigates_to": "project_creation"
        },
        "navigate_to_team_members": {
          "description": "Navigate to team members page",
          "steps": [
            {
              "type": "click",
              "selector": "button:contains('Team')",
              "fallback_selectors": [
                "//button[contains(text(), 'Team')]",
                "a[href='/team-members']"
              ],
              "description": "Click Team button in header"
            },
            {
              "type": "wait",
              "seconds": 1
            }
          ],
          "navigates_to": "team_members"
        },
        "open_first_project": {
          "description": "Open the first project card",
          "steps": [
            {
              "type": "click",
              "selector": ".card:first-of-type",
              "fallback_selectors": [
                "div.card:first-child",
                "//main//div[contains(@class, 'card')][1]"
              ],
              "description": "Click on first project card"
            },
            {
              "type": "wait",
              "seconds": 2
            }
          ],
          "navigates_to": "project_dashboard"
        },
        "count_projects": {
          "description": "Count the number of projects on dashboard",
          "steps": [
            {
              "type": "count_elements",
              "selector": ".card",
              "fallback_selectors": [
                "div.card",
                "//main//div[contains(@class, 'card')]"
              ],
              "description": "Count project cards"
            }
          ]
        }
      }
    },
    "project_creation": {
      "url": "http://localhost:5173/create-project",
      "identifiers": {
        "url_pattern": "/create-project",
        "elements": ["input", "button"]
      },
      "description": "AI-powered project creation page with chat interface",
      "actions": {
        "create_project_with_ai": {
          "description": "Create a project by chatting with AI",
          "steps": [
            {
              "type": "input",
              "selector": "input[placeholder*='message']",
              "fallback_selectors": [
                "input[type='text']",
                "textarea"
              ],
              "param": "description",
              "description": "Enter project description in chat input"
            },
            {
              "type": "click",
              "selector": "button[type='submit']",
              "fallback_selectors": [
                "button:contains('Send')",
                "//button[contains(text(), 'Send')]",
                "form button"
              ],
              "description": "Click send button"
            },
            {
              "type": "wait",
              "seconds": 5,
              "description": "Wait for AI to process and create project"
            }
          ],
          "navigates_to": "dashboard"
        }
      }
    },
    "project_dashboard": {
      "url": "http://localhost:5173/project/{project_id}",
      "identifiers": {
        "url_pattern": "/project/[a-f0-9-]+$",
        "elements": ["button", ".card"]
      },
      "description": "Individual project dashboard with stats and recent tasks",
      "actions": {
        "navigate_to_task_board": {
          "description": "Navigate to task board from project",
          "steps": [
            {
              "type": "click",
              "selector": "button:contains('View Task Board')",
              "fallback_selectors": [
                "//button[contains(text(), 'View Task Board')]",
                "//button[contains(text(), 'Task Board')]",
                "a[href='/task-board']"
              ],
              "description": "Click View Task Board button"
            },
            {
              "type": "wait",
              "seconds": 1
            }
          ],
          "navigates_to": "task_board"
        },
        "navigate_to_issues": {
          "description": "Navigate to issues page",
          "steps": [
            {
              "type": "click",
              "selector": "button:contains('Issues')",
              "fallback_selectors": [
                "//button[contains(text(), 'Issues')]",
                "//button[contains(., 'üêõ')]",
                "//span[text()='Issues']/parent::button"
              ],
              "description": "Click Issues button"
            },
            {
              "type": "wait",
              "seconds": 1
            }
          ],
          "navigates_to": "issues_page"
        },
        "navigate_to_epics": {
          "description": "Navigate to epics page",
          "steps": [
            {
              "type": "click",
              "selector": "button:contains('Epics')",
              "fallback_selectors": [
                "//button[contains(text(), 'Epics')]",
                "//button[contains(., 'üìö')]"
              ],
              "description": "Click Epics button"
            },
            {
              "type": "wait",
              "seconds": 1
            }
          ],
          "navigates_to": "epics_page"
        },
        "navigate_to_risks": {
          "description": "Navigate to risk dashboard",
          "steps": [
            {
              "type": "click",
              "selector": "button:contains('Risks')",
              "fallback_selectors": [
                "//button[contains(text(), 'Risks')]",
                "//button[contains(., '‚ö†Ô∏è')]"
              ],
              "description": "Click Risks button"
            },
            {
              "type": "wait",
              "seconds": 1
            }
          ],
          "navigates_to": "risk_dashboard"
        }
      }
    },
    "task_board": {
      "url": "http://localhost:5173/task-board",
      "identifiers": {
        "url_pattern": "/task-board",
        "elements": [".task-card", "select"]
      },
      "description": "Kanban-style task board with To Do, In Progress, Done columns",
      "actions": {
        "start_first_task": {
          "description": "Start the first task in To Do column",
          "steps": [
            {
              "type": "click",
              "selector": "button:contains('Start Task')",
              "fallback_selectors": [
                "//button[contains(text(), 'Start Task')]",
                "//button[contains(text(), 'Start')]"
              ],
              "description": "Click Start Task button on first task"
            },
            {
              "type": "wait",
              "seconds": 1
            }
          ]
        },
        "complete_first_task": {
          "description": "Complete the first task in In Progress column",
          "steps": [
            {
              "type": "click",
              "selector": "button:contains('Mark Complete')",
              "fallback_selectors": [
                "//button[contains(text(), 'Mark Complete')]",
                "//button[contains(text(), 'Complete')]"
              ],
              "description": "Click Mark Complete button on first task"
            },
            {
              "type": "wait",
              "seconds": 1
            }
          ]
        }
      }
    },
    "team_members": {
      "url": "http://localhost:5173/team-members",
      "identifiers": {
        "url_pattern": "/team-members",
        "elements": ["input[type='email']", "button"]
      },
      "description": "Team members management page",
      "actions": {
        "add_team_member": {
          "description": "Add a new team member",
          "steps": [
            {
              "type": "click",
              "selector": "button:contains('Add Member')",
              "fallback_selectors": [
                "//button[contains(text(), 'Add Member')]",
                "//button[contains(text(), 'Add')]"
              ],
              "description": "Click Add Member button"
            },
            {
              "type": "wait",
              "seconds": 1
            },
            {
              "type": "input",
              "selector": "input[name='username']",
              "fallback_selectors": [
                "input[placeholder*='username' i]",
                "//input[@type='text'][1]"
              ],
              "param": "username",
              "description": "Enter username"
            },
            {
              "type": "input",
              "selector": "input[type='email']",
              "fallback_selectors": [
                "input[name='email']",
                "input[placeholder*='email' i]"
              ],
              "param": "email",
              "description": "Enter email"
            },
            {
              "type": "input",
              "selector": "input[type='password']",
              "fallback_selectors": [
                "input[name='password']",
                "input[placeholder*='password' i]"
              ],
              "param": "password",
              "description": "Enter password"
            },
            {
              "type": "select",
              "selector": "select[name='role']",
              "fallback_selectors": [
                "select",
                "//select[1]"
              ],
              "param": "role",
              "description": "Select role"
            },
            {
              "type": "click",
              "selector": "button[type='submit']",
              "fallback_selectors": [
                "button:contains('Submit')",
                "//button[contains(text(), 'Submit')]",
                "form button"
              ],
              "description": "Click Submit button"
            },
            {
              "type": "wait",
              "seconds": 2
            }
          ]
        }
      }
    },
    "issues_page": {
      "url": "http://localhost:5173/project/{project_id}/issues",
      "identifiers": {
        "url_pattern": "/project/[a-f0-9-]+/issues",
        "elements": ["button", ".card"]
      },
      "description": "Issues and bugs tracking page",
      "actions": {
        "create_issue": {
          "description": "Create a new issue",
          "steps": [
            {
              "type": "click",
              "selector": "button:contains('Report Issue')",
              "fallback_selectors": [
                "//button[contains(text(), 'Report Issue')]",
                "//button[contains(text(), 'Create Issue')]"
              ],
              "description": "Click Report Issue button"
            },
            {
              "type": "wait",
              "seconds": 1
            },
            {
              "type": "input",
              "selector": "input[name='title']",
              "fallback_selectors": [
                "input[placeholder*='title' i]",
                "//input[@type='text'][1]"
              ],
              "param": "title",
              "description": "Enter issue title"
            },
            {
              "type": "input",
              "selector": "textarea[name='description']",
              "fallback_selectors": [
                "textarea",
                "//textarea[1]"
              ],
              "param": "description",
              "description": "Enter issue description"
            },
            {
              "type": "select",
              "selector": "select[name='type']",
              "fallback_selectors": [
                "//select[1]"
              ],
              "param": "type",
              "description": "Select issue type"
            },
            {
              "type": "select",
              "selector": "select[name='priority']",
              "fallback_selectors": [
                "//select[2]"
              ],
              "param": "priority",
              "description": "Select priority"
            },
            {
              "type": "click",
              "selector": "button[type='submit']",
              "fallback_selectors": [
                "button:contains('Submit')",
                "//button[contains(text(), 'Submit')]"
              ],
              "description": "Click Submit button"
            },
            {
              "type": "wait",
              "seconds": 2
            }
          ]
        }
      }
    }
  }
}
